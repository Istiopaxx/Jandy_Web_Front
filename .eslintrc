{
    "plugins": [
        "prettier"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "ignorePatterns": [
        "node_modules/"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
